{
	"data": {
		"train_filename": "rul_data.csv",
		"columns_X": ["RUL"],
		"columns_Y": ["RUL"],
		"sequence_length": 200,
		"train_test_split": 0.9,
		"normalise": true
	},
	"training": {
		"epochs": 1,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "..\\save\\lstm_check_point",
		"model_dir": "..\\save\\lstm_model",
		"logs_dir": "..\\save\\lstm_tensor_board",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 199,
				"input_dim": 1,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}